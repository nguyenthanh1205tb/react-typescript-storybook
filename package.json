  "license": "MIT",
  "types": "build/index.d.ts",
  "main": "build/index.js",
  "typings": "build/index.d.ts",
  "files": [
    "build"
  ],
  "scripts": {
    "start:sb": "storybook dev -p 5050",
    "build:sb": "yarn build:tw && run-p watch:tw start:sb",
    "build:tw": "postcss src/css/index.src.css -o src/css/index.css",
    "watch:tw": "postcss -w src/css/index.src.css -o src/css/index.css",
    "watch:rollup": "rollup -c rollup.config.js --bundleConfigAsCjs --watch",
    "dev": "run-p watch:tw watch:rollup",
    "build": "export NODE_ENV=production && rollup -c rollup.config.js --bundleConfigAsCjs",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "resolutions": {
    "string-width": "4.2.3"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/preset-env": "^7.24.0",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@storybook/addon-essentials": "^7.6.17",
    "@storybook/addon-webpack5-compiler-babel": "^3.0.0",
    "@storybook/addons": "^7.6.17",
    "@storybook/react": "^7.6.17",
    "@storybook/react-webpack5": "^7.6.17",
    "@storybook/theming": "^7.6.17",
    "@tailwindcss/custom-forms": "^0.2.1",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@types/jest": "^29.5.12",
    "@types/react": "^18.2.61",
    "@types/react-dom": "^18.2.19",
    "@types/storybook__react": "^5.2.1",
    "babel-loader": "^9.1.3",
    "cssnano": "^6.0.5",
    "jest": "^29.7.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.35",
    "postcss-cli": "^11.0.0",
    "react-docgen-typescript": "^2.2.2",
    "rollup": "^4.12.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.36.0",
    "storybook": "^7.6.17",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "react": ">=16.8",
    "react-dom": ">=16.8"
  },
  "dependencies": {
    "autoprefixer": "9.8.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
